        <activity android:name=".remainder.PreferenceSettingsOneActivity" />
        <activity android:name=".remainder.PreferenceSettingsTwoActivity"/>
        <activity
            android:name=".total.MaterialGirlGroupActivity"
            android:theme="@style/GirlGroupTheme" />
        <activity android:name=".total.GirlsMemberDetailActivity"
            android:theme="@style/GirlGroupTheme"
            android:parentActivityName=".total.MaterialGirlGroupActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".total.MaterialGirlGroupActivity" />
        </activity>
    </application>
</manifest>