        android:theme="@style/Theme.MapoTheme">

        <activity android:name=".materialwidget.CoordinatorLayoutActivity" />
        <activity android:name=".materialwidget.TabLayoutBasicActivity" />
        <activity android:name=".MainListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".materialbase.ElevationApplyActivity" />
        <activity android:name=".MaterialBasicWidgetList" />
        <activity android:name=".materialwidget.MaterialTextAndButtonActivity" />
        <activity android:name=".materialbase.RevealEffectActivity" />
        <activity android:name=".materialbase.RippleAnimationActivity" />
