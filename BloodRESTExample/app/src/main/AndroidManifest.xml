<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.co.pyo.rest">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.BloodRESTExample"
        tools:ignore="UnusedAttribute">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".okhttp.BloodRESTActivity"
            android:windowSoftInputMode="adjustPan|stateAlwaysVisible" />
        <activity android:name=".retrofit.BloodRetrofitRESTActivity"
            android:windowSoftInputMode="adjustPan|stateAlwaysVisible" />
        <activity android:name=".okhttp_retrofit.BloodOkHttpRetrofitRESTActivity"
                    android:windowSoftInputMode="adjustPan|stateAlwaysVisible" />
        <activity android:name=".glide.GlideViewPagerActivity" />
    </application>

</manifest>